<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@color/Blue"
        android:layout_height="50dp">

        <ImageView
            android:id="@+id/btnBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:background="@mipmap/icon_back_arrow"
            android:visibility="visible" />

        <TextView
            android:id="@+id/txtHeader"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/btnBack"
            android:text="Setting"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FFFFFF"
            android:visibility="visible" />
    </RelativeLayout>
    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/gray"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/change_pwd">

            <TextView
                android:id="@+id/txt_tabtxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_tabtxt"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Change password"
                android:textColor="@android:color/darker_gray"

                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_tabtxt"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_changepwd"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>
        <View
            android:background="@color/gray"
            android:layout_width="1dp"
            android:layout_gravity="center_horizontal"
            android:layout_height="match_parent"/>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/change_mob">

            <TextView
                android:id="@+id/txt_mylist"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_mylist"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Change mobile no."
                android:textColor="@android:color/darker_gray"

                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_mylist"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_mobile"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>

    </LinearLayout>
    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"

        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/gray"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/comp_prof">

            <TextView
                android:id="@+id/txt_barter_recived"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_barter_recived"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Complete Profile"
                android:textColor="@android:color/darker_gray"

                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_barter_recived"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_action_profile"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>
        <View
            android:background="@color/gray"
            android:layout_width="1dp"
            android:layout_gravity="center_horizontal"
            android:layout_height="match_parent"/>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/notification">

            <TextView
                android:id="@+id/txt_barter_made"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_barter_made"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Notifications"
                android:textColor="@android:color/darker_gray"

                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_barter_made"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_action_notification"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>

    </LinearLayout>
    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"

        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/gray"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2"

        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/share" >

            <TextView
                android:id="@+id/txt_following"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_following"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Share Opinion"
                android:textColor="@android:color/darker_gray"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_following"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_action_shareopinion"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>
        <RelativeLayout
            android:background="@color/gray"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/howto" >

            <TextView
                android:id="@+id/txt_follower"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_follower"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="How to Use"
                android:textColor="@android:color/darker_gray"

                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_follower"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_action_howtouse"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>

    </LinearLayout>
    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"

        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/gray"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2"

        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/about">

            <TextView
                android:id="@+id/txt_i_look_for"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_i_look_for"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="About Us "
                android:textColor="@android:color/darker_gray"

                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_i_look_for"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_action_aboutus"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>
        <RelativeLayout
            android:background="@color/gray"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:id="@+id/terms">

            <TextView
                android:id="@+id/txt_like"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_like"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Terms and Conditions"
                android:textColor="@android:color/darker_gray"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_like"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_terms"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>

    </LinearLayout>
    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/gray"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:orientation="vertical"
            android:id="@+id/who">

            <TextView
                android:id="@+id/txt_who_is"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_who_is"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Who is"
                android:textColor="@android:color/darker_gray"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_who_is"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_action_who"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>
        <RelativeLayout
            android:background="@color/gray"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:id="@+id/logout">

            <TextView
                android:id="@+id/txt_logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_logout"
                android:layout_marginTop="2dp"
                android:textSize="13dp"
                android:layout_centerHorizontal="true"
                android:text="Logout"
                android:textColor="@android:color/darker_gray"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <ImageView
                android:id="@+id/img_logout"
                android:layout_width="33dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_logout"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"

                />

        </RelativeLayout>

    </LinearLayout>
    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/gray"/>
   <!-- <Button
        android:id="@+id/logout"
        android:layout_gravity="center"
        android:text="Logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />-->



</LinearLayout>